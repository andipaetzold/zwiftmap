{
  "name": "ZwiftMap for Strava",
  "description": "Test",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.strava.com/activities/*"],
      "js": ["./dist/inject.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://www.strava.com/*"],
      "resources": ["dist/script.js", "assets/*"]
    }
  ]
}
