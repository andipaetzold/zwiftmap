{
  "name": "@zwiftmap/backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "start": "concurrently -n web,worker npm:start:web npm:start:worker",
    "start:web": "node ./build-web/web",
    "start:worker": "node ./build-worker/worker",
    "dev": "concurrently -n web,worker npm:dev:web npm:dev:worker",
    "dev:web": "node ./scripts/start-web.mjs",
    "dev:worker": "tsc-watch -P worker.tsconfig.json --preserveWatchOutput --onSuccess \"node ./build-worker/worker\"",
    "build": "npm run build:web && npm run build:worker",
    "build:web": "tsc -P web.tsconfig.json",
    "build:worker": "tsc -P worker.tsconfig.json",
    "pg-migrate": "node-pg-migrate"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/bull": "3.15.8",
    "@types/compression": "1.7.2",
    "@types/connect-redis": "0.0.18",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/lodash": "4.14.182",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.36",
    "@types/pg": "8.6.5",
    "@types/redis": "2.8.32",
    "@types/sharp": "0.30.2",
    "@types/tsc-watch": "4.2.0",
    "@types/validator": "13.7.2",
    "ngrok": "4.3.1",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.1-rc"
  },
  "dependencies": {
    "@google-cloud/storage": "5.19.4",
    "@sentry/node": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "axios": "0.27.2",
    "bull": "4.8.3",
    "compression": "1.7.4",
    "concurrently": "7.2.0",
    "connect-redis": "6.1.3",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "fast-xml-parser": "4.0.7",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "nocache": "3.0.3",
    "node-pg-migrate": "6.2.1",
    "pg": "8.7.3",
    "redis": "3.1.2",
    "runtypes": "6.5.1",
    "sharp": "0.30.4",
    "short-uuid": "4.2.0",
    "source-map-support": "0.5.21",
    "strava": "2.0.1",
    "xmlbuilder2": "3.0.2",
    "zwift-data": "1.16.0"
  }
}
