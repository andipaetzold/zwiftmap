{
  "name": "@zwiftmap/backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.17.0"
  },
  "scripts": {
    "start": "concurrently -n web,worker npm:start:web npm:start:worker",
    "start:web": "node ./build-web/web",
    "start:worker": "node ./build-worker/worker",
    "dev": "concurrently -n web,worker npm:dev:web npm:dev:worker",
    "dev:web": "node ./scripts/start-web.mjs",
    "dev:worker": "tsc-watch -P worker.tsconfig.json --preserveWatchOutput --onSuccess \"node ./build-worker/worker\"",
    "build": "npm run build:web && npm run build:worker",
    "build:web": "tsc -P web.tsconfig.json",
    "build:worker": "tsc -P worker.tsconfig.json",
    "pg-migrate": "node-pg-migrate"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/bull": "3.15.9",
    "@types/compression": "1.7.2",
    "@types/connect-redis": "0.0.18",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.5",
    "@types/lodash": "4.14.184",
    "@types/lz-string": "1.3.34",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.49",
    "@types/pg": "8.6.5",
    "@types/redis": "2.8.32",
    "@types/sharp": "0.30.5",
    "@types/tsc-watch": "4.2.0",
    "@types/validator": "13.7.5",
    "ngrok": "4.3.3",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@google-cloud/firestore": "6.0.0",
    "@google-cloud/storage": "6.4.1",
    "@sentry/node": "7.11.1",
    "@sentry/tracing": "7.11.1",
    "axios": "0.27.2",
    "bull": "4.8.5",
    "compression": "1.7.4",
    "concurrently": "7.3.0",
    "connect-redis": "6.1.3",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "fast-xml-parser": "4.0.9",
    "lodash": "4.17.21",
    "lz-string": "1.4.4",
    "morgan": "1.10.0",
    "nocache": "3.0.4",
    "node-pg-migrate": "6.2.2",
    "pg": "8.7.3",
    "redis": "3.1.2",
    "runtypes": "6.6.0",
    "sharp": "0.30.7",
    "short-uuid": "4.2.0",
    "source-map-support": "0.5.21",
    "strava": "2.0.1",
    "xmlbuilder2": "3.0.2",
    "zwift-data": "1.17.0"
  }
}
