{
  "name": "zwiftmap-backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.13.1"
  },
  "scripts": {
    "start:web": "node ./build-web/web",
    "start:worker": "node ./build-worker/worker",
    "dev:web": "node ./scripts/start-web.mjs",
    "dev:worker": "tsc-watch -P worker.tsconfig.json --preserveWatchOutput --onSuccess \"node ./build-worker/worker\"",
    "build": "npm run build:web && npm run build:worker",
    "build:web": "tsc -P web.tsconfig.json",
    "build:worker": "tsc -P worker.tsconfig.json",
    "pg-migrate": "node-pg-migrate"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/bull": "3.15.7",
    "@types/compression": "1.7.2",
    "@types/connect-redis": "0.0.18",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/lodash": "4.14.178",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.12",
    "@types/node-fetch": "2.5.3",
    "@types/pg": "8.6.1",
    "@types/redis": "2.8.32",
    "@types/tsc-watch": "4.2.0",
    "@types/validator": "13.7.0",
    "ngrok": "4.2.2",
    "tsc-watch": "4.5.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@sentry/node": "6.16.0",
    "@sentry/tracing": "6.16.0",
    "axios": "0.24.0",
    "bull": "4.1.1",
    "cloudinary": "1.27.1",
    "compression": "1.7.4",
    "connect-redis": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "fast-xml-parser": "4.0.0-beta.6",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "nocache": "3.0.1",
    "node-pg-migrate": "6.0.0",
    "pg": "8.7.1",
    "puppeteer": "12.0.1",
    "redis": "3.1.2",
    "runtypes": "6.5.0",
    "short-uuid": "4.2.0",
    "source-map-support": "0.5.21",
    "strava": "2.0.1",
    "xmlbuilder2": "3.0.2",
    "zwift-data": "1.10.3"
  }
