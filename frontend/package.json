{
  "name": "zwiftmap-frontend",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@mapbox/polyline": "1.1.1",
    "@react-leaflet/core": "1.0.2",
    "@react-md/alert": "5.0.0",
    "@react-md/avatar": "5.0.0",
    "@react-md/button": "5.0.0",
    "@react-md/chip": "5.0.0",
    "@react-md/dialog": "5.0.0",
