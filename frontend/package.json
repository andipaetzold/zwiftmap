  "scripts": {
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "prebuild": "npm run prebuild:fetch-segments && npm run prebuild:sitemap && npm run prebuild:assets",
    "prebuild:fetch-segments": "node scripts/fetch-segments.mjs",
    "prebuild:sitemap": "node scripts/create-sitemap.mjs",
    "prebuild:assets": "pwa-asset-generator ./public/assets/logo.svg ./public/assets -i ./public/index.html -m ./public/manifest.webmanifest -o true -t png",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test",
    "deploy": "netlify deploy"
  },
  "type": "module",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
