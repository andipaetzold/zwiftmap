  "scripts": {
    "prepare": "cd .. && husky install frontend/.husky",
    "start": "npm run dev",
    "dev": "VITE_GIT_SHA=`git rev-parse --short HEAD` vite --host",
    "prebuild": "npm run prebuild:fetch-segments && npm run prebuild:sitemap && npm run prebuild:world-roads-buffered && npm run prebuild:assets",
    "prebuild:fetch-segments": "node --loader ts-node/esm ./scripts/fetch-segments.ts",
    "prebuild:sitemap": "node --loader ts-node/esm ./scripts/create-sitemap.ts",
    "prebuild:world-roads-buffered": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/create-world-roads-buffered.ts",
    "prebuild:assets": "pwa-asset-generator ./public/assets/logo.svg ./public/assets -v /assets -i ./index.html -m ./public/manifest.webmanifest -o true -t png",
    "build": "npm run build:tsc && npm run build:react",
    "build:tsc": "tsc",
    "build:react": "VITE_GIT_SHA=`git rev-parse --short HEAD` vite build",
    "test": "vitest",
    "deploy": "netlify deploy",
    "lint": "eslint src",
    "lint-staged": "lint-staged"
  },
  "type": "module",
  "devDependencies": {
    "@types/cli-progress": "3.11.4",
    "@types/file-saver": "2.0.6",
    "@types/lodash-es": "4.17.10",
    "@types/mapbox__polyline": "1.0.4",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "@vitejs/plugin-react": "4.1.0",
    "cli-progress": "3.12.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "12.9.1",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "netlify-cli": "12.2.8",
    "prettier": "3.0.3",
    "pwa-asset-generator": "6.3.1",
    "sass": "1.69.4",
    "schema-dts": "1.1.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-plugin-pwa": "0.16.5",
    "vitest": "0.34.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ]
  }
}
