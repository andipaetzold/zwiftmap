    "@react-md/menu": "5.0.0",
    "@react-md/progress": "5.0.0",
    "@react-md/typography": "5.0.0",
    "@sentry/react": "6.18.1",
    "@sentry/tracing": "6.18.1",
    "@turf/boolean-point-in-polygon": "6.5.0",
    "@turf/distance": "6.5.0",
    "@turf/helpers": "6.5.0",
    "@turf/length": "6.5.0",
    "@turf/nearest-point-on-line": "6.5.0",
    "@types/leaflet": "1.7.9",
    "@types/node": "16.11.26",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "classnames": "2.3.1",
    "comlink": "4.3.1",
    "date-fns": "2.28.0",
    "fast-xml-parser": "4.0.5",
    "file-saver": "2.0.5",
    "leaflet": "1.7.1",
    "lodash-es": "4.17.21",
    "mitt": "3.0.0",
    "node-fetch": "3.2.1",
    "react": "17.0.2",
    "react-async-hook": "4.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.3",
    "react-leaflet": "3.1.0",
    "react-responsive": "9.0.0-beta.6",
    "react-schemaorg": "2.0.0",
    "recharts": "2.1.9",
    "strava": "2.0.1",
    "zustand": "3.7.1",
    "zwift-data": "1.13.2"
  },
  "scripts": {
    "start": "VITE_GIT_SHA=`git rev-parse --short HEAD` vite",
    "prebuild": "npm run prebuild:fetch-segments && npm run prebuild:sitemap && npm run prebuild:assets",
    "prebuild:fetch-segments": "node scripts/fetch-segments.mjs",
    "prebuild:sitemap": "node scripts/create-sitemap.mjs",
    "prebuild:assets": "pwa-asset-generator ./public/assets/logo.svg ./public/assets -v /assets -i ./index.html -m ./public/manifest.webmanifest -o true -t png",
    "build": "tsc --noEmit && VITE_GIT_SHA=`git rev-parse --short HEAD` vite build",
    "test": "vitest",
    "deploy": "netlify deploy"
  },
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "1.0.2",
    "@types/file-saver": "2.0.5",
    "@types/lodash-es": "4.17.6",
    "@types/mapbox__polyline": "1.0.2",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "cli-progress": "3.10.0",
    "happy-dom": "2.45.0",
    "netlify-cli": "9.12.3",
    "prettier": "2.5.1",
    "pwa-asset-generator": "6.0.6",
    "sass": "1.49.9",
    "schema-dts": "1.1.0",
    "typescript": "4.6.2",
    "vite": "2.8.6",
    "vitest": "0.5.9"
  }
}
